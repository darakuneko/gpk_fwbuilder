{
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./public/build && rm -rf ./build && rm -rf ./dist && npx webpack",
    "build:win": "del /s /q public\\build && del /s /q build && del /s /q dist && npx webpack",
    "start": "npx webpack && npx electron .",
    "dist:win": "npm run build:win && electron-builder -w",
    "dist:mac": "npm run build && electron-builder -m",
    "dist:linux": "npm run build && electron-builder -l"
  },
  "name": "gpk_fwbuilder",
  "version": "0.10.3",
  "description": "GPK FWBuilder",
  "homepage": "https://github.com/darakuneko",
  "author": {
    "name": "darakuneko",
    "email": "tvkids@gmail.com"
  },
  "devDependencies": {
    "@electron/rebuild": "3.6.0",
    "electron": "29.0.0",
    "electron-builder": "24.12.0",
    "webpack-cli": "5.1.4"
  },
  "main": "./index.js",
  "dependencies": {
    "@babel/core": "7.23.9",
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@emotion/babel-preset-css-prop": "11.11.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.15.10",
    "@mui/material": "5.15.10",
    "ansi-to-html": "0.7.2",
    "axios": "1.6.7",
    "babel-loader": "9.1.3",
    "electron-store": "8.1.0",
    "form-data": "4.0.0",
    "html-react-parser": "5.1.7",
    "node-loader": "2.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "webpack": "5.90.3"
  },
  "build": {
    "productName": "GPK FWBuilder",
    "appId": "app.darakuneko.gpk_fwbuilder",
    "npmRebuild": false,
    "files": [
      "public/**/*",
      "icons/*",
      "index.js",
      "preload.js",
      "command.js",
      "package.json"
    ],
    "extraResources": [
      "gpk_fwmaker/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icons/512x512.png"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "icons"
    }
  }
}
